pipeline {
    agent none
    stages {
        stage('Code Checkout'){
            echo "This is code checkout stage"
        }

        stage('Image Build Stage'){
            echo "This is image build stage"
        }

        stage('Image Push Stage'){
            echo "This is image push stage"
        }

        stage('Push Code to K8 manifest repo'){
            echo "This is last stage"
        }
    }
}
